{
    "structure": "Relationnet",
    "train_dir": "datasets/task2-dataset",
    "test_dir": "datasets/test",
    "save": {
        "dir": "relation_5_shot-v01",
        "save_freq": 10
    },
    "sampler": {
        "train": {
            "n_way": 20,
            "k_shot": 5,
            "k_query": 15,
            "episodes": 1000
        },
        "test": {
            "n_way": 20,
            "k_shot": 5,
            "k_query": 15,
            "episodes": 500
        }
    },
    "epochs": 500,
    "model": {
        "hidden_size": 64,
        "feature_size": 64,
        "relation_dim": 8
    },
    "optimizer": {
        "lr": 0.001,
        "scheduler": {
            "gamma": 0.5,
            "step_size": 100
        }
    },
    "metric": "accuracy",
    "cuda": true
}

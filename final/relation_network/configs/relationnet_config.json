{
    "structure": "Relationnet",
    "base_dir": "datasets/task2-dataset/base",
    "novel_dir": "datasets/task2-dataset/novel",
    "test_dir": "datasets/test",
    "save": {
        "dir": "relation_5_shot-v01",
        "save_freq": 1000
    },
    "train": {
        "sample": {
            "n_way": 20,
            "k_shot": 5,
            "n_query": 5,
            "novel_index": [1, 2, 3, 4, 5]
        },
        "transform": {
            "horizontal_flip": 0.1,
            "vertical_flip": 0.1,
            "rotate_degree": 15
        },
        "episode": 500000
    },
    "model": {
        "embedder": {
            "hidden_size": [64, 64, 64]
        },
        "feature_size": 64,
        "relation": {
            "hidden_size": [64, 64]
        },
        "relation_dim": 8
    },
    "optimizer": {
        "lr": 0.001,
        "scheduler": {
            "gamma": 0.5,
            "step_size": 100000
        }
    },
    "log_step": 100,
    "eval_step": 1000,
    "metric": "accuracy",
    "cuda": true
}
